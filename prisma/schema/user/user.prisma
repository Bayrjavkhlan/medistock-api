model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String
  name      String?

  roles Role[]

  hospital Hospital?     @relation(fields: [hospitalId], references: [id])
  hospitalId String?

  createdBy String?
  updatedBy String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}