model EquipmentLog {
  id          String    @id @default(uuid())
  equipment   Equipment @relation(fields: [equipmentId], references: [id])
  equipmentId String

  performedBy Staff  @relation("PerformedLogs", fields: [staffId], references: [id])
  staffId     String

  description String

  createdBy String?
  updatedBy String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
